### Basic settings
#
set $mod		Mod4
set $Alt		Mod1
set $term		term
set $terminal		term
set $terminalfl		termf
set $lockscreen		$HOME/bin/swaylock.sh
set $suspend		systemctl suspend
set $lock		loginctl lock-session

# window parameters
set $defaultborder	pixel 4
set $noborder		pixel 0
set $opacity		0.9

# workspaces
set $ws_www		1
set $ws_chat		0
set $ws_util    	12
set $ws_music		11
#set $ws_
