_clusterctl_describe_cluster()
{
    last_command="clusterctl_describe_cluster"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--color")
    flags+=("-c")
    local_nonpersistent_flags+=("--color")
    local_nonpersistent_flags+=("-c")
    flags+=("--echo")
    local_nonpersistent_flags+=("--echo")
    flags+=("--grouping")
    local_nonpersistent_flags+=("--grouping")
    flags+=("--kubeconfig=")
    two_word_flags+=("--kubeconfig")
    local_nonpersistent_flags+=("--kubeconfig")
    local_nonpersistent_flags+=("--kubeconfig=")
    flags+=("--kubeconfig-context=")
    two_word_flags+=("--kubeconfig-context")
    flags_with_completion+=("--kubeconfig-context")
    flags_completion+=("__clusterctl_handle_go_custom_completion")
