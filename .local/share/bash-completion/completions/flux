    two_word_flags+=("--toleration-keys")
    flags+=("--user=")
    two_word_flags+=("--user")
    flags+=("--verbose")
    flags+=("--version=")
    two_word_flags+=("--version")
    two_word_flags+=("-v")
    flags+=("--watch-all-namespaces")

    must_have_one_flag=()
    must_have_one_noun=()
