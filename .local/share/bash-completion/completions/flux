{
    last_command="flux_diff"

    command_aliases=()

    commands=()
    commands+=("artifact")
    commands+=("kustomization")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("ks")
        aliashash["ks"]="kustomization"
    fi

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--as=")
    two_word_flags+=("--as")
