    flags+=("--as-group=")
    two_word_flags+=("--as-group")
    local_nonpersistent_flags+=("--as-group")
    local_nonpersistent_flags+=("--as-group=")
    flags+=("--as-uid=")
    two_word_flags+=("--as-uid")
    local_nonpersistent_flags+=("--as-uid")
    local_nonpersistent_flags+=("--as-uid=")
