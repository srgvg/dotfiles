_cmctl_root_command()
{
    last_command="cmctl"

    command_aliases=()

    commands=()
    commands+=("approve")
    commands+=("check")
    commands+=("completion")
    commands+=("convert")
    commands+=("create")
    commands+=("deny")
    commands+=("experimental")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("x")
        aliashash["x"]="experimental"
    fi
    commands+=("help")
    commands+=("inspect")
    commands+=("renew")
    commands+=("status")
    commands+=("upgrade")
    commands+=("version")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--log-flush-frequency=")
    two_word_flags+=("--log-flush-frequency")
    flags+=("--logging-format=")
    two_word_flags+=("--logging-format")
    flags+=("--v")
    flags+=("-v")
    flags+=("--vmodule=")
    two_word_flags+=("--vmodule")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

__start_cmctl()
{
    local cur prev words cword split
    declare -A flaghash 2>/dev/null || :
    declare -A aliashash 2>/dev/null || :
    if declare -F _init_completion >/dev/null 2>&1; then
        _init_completion -s || return
    else
        __cmctl_init_completion -n "=" || return
    fi

    local c=0
    local flag_parsing_disabled=
    local flags=()
    local two_word_flags=()
    local local_nonpersistent_flags=()
    local flags_with_completion=()
    local flags_completion=()
    local commands=("cmctl")
    local command_aliases=()
    local must_have_one_flag=()
    local must_have_one_noun=()
    local has_completion_function=""
    local last_command=""
    local nouns=()
    local noun_aliases=()

    __cmctl_handle_word
}

if [[ $(type -t compopt) = "builtin" ]]; then
    complete -o default -F __start_cmctl cmctl
else
    complete -o default -o nospace -F __start_cmctl cmctl
fi

# ex: ts=4 sw=4 et filetype=sh
