    last_command="forward-email_domain_update"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--adult-content-protection")
    local_nonpersistent_flags+=("--adult-content-protection")
    flags+=("--caldav-port=")
    two_word_flags+=("--caldav-port")
    local_nonpersistent_flags+=("--caldav-port")
    local_nonpersistent_flags+=("--caldav-port=")
    flags+=("--carddav-port=")
    two_word_flags+=("--carddav-port")
    local_nonpersistent_flags+=("--carddav-port")
    local_nonpersistent_flags+=("--carddav-port=")
    flags+=("--executable-protection")
    local_nonpersistent_flags+=("--executable-protection")
    flags+=("--imap-port=")
    two_word_flags+=("--imap-port")
    local_nonpersistent_flags+=("--imap-port")
    local_nonpersistent_flags+=("--imap-port=")
    flags+=("--max-forwarded-addresses=")
    two_word_flags+=("--max-forwarded-addresses")
    local_nonpersistent_flags+=("--max-forwarded-addresses")
    local_nonpersistent_flags+=("--max-forwarded-addresses=")
    flags+=("--phishing-protection")
    local_nonpersistent_flags+=("--phishing-protection")
    flags+=("--retention-days=")
    two_word_flags+=("--retention-days")
    local_nonpersistent_flags+=("--retention-days")
    local_nonpersistent_flags+=("--retention-days=")
    flags+=("--smtp-port=")
    two_word_flags+=("--smtp-port")
    local_nonpersistent_flags+=("--smtp-port")
    local_nonpersistent_flags+=("--smtp-port=")
    flags+=("--virus-protection")
    local_nonpersistent_flags+=("--virus-protection")
    flags+=("--webhook-url=")
    two_word_flags+=("--webhook-url")
    local_nonpersistent_flags+=("--webhook-url")
    local_nonpersistent_flags+=("--webhook-url=")
    flags+=("--debug")
    flags+=("--output=")
    two_word_flags+=("--output")
    two_word_flags+=("-o")
    flags+=("--profile=")
    two_word_flags+=("--profile")
    two_word_flags+=("-p")
    flags+=("--timeout=")
    two_word_flags+=("--timeout")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

