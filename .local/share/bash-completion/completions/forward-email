_forward-email_alias_recipients()
{
    last_command="forward-email_alias_recipients"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--recipients=")
    two_word_flags+=("--recipients")
    local_nonpersistent_flags+=("--recipients")
