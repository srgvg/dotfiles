    flags+=("--custom-cni-url=")
    two_word_flags+=("--custom-cni-url")
    local_nonpersistent_flags+=("--custom-cni-url")
    local_nonpersistent_flags+=("--custom-cni-url=")
    flags+=("--disable-dhcp-hostname")
    local_nonpersistent_flags+=("--disable-dhcp-hostname")
